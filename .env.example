# Environment Variables for Dubai Automation Dashboard
# Copy this to .env and fill in your values

# API Configuration
VITE_API_URL=http://localhost:3001
VITE_APP_NAME=AI Real Estate Team

# Database Configuration (for development)
VITE_DATABASE_URL=postgresql://automation_user:password@automation-postgres:5432/automation_platform

# Supabase Configuration (when ready)
VITE_SUPABASE_URL=https://supabase.yasta.online
VITE_SUPABASE_ANON_KEY=your_anon_key_here
VITE_SUPABASE_SERVICE_ROLE_KEY=your_service_key_here

# Client Configuration
VITE_CLIENT_ID=demo_client
VITE_DEFAULT_LANGUAGE=en

# Development Settings
VITE_MOCK_DATA=true
VITE_DEBUG=true