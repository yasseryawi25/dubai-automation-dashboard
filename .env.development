# Environment Variables for Dubai AI Real Estate Agent Platform - DEVELOPMENT
# This version points to real backend API server for testing

# ==============================================
# APPLICATION CONFIGURATION
# ==============================================
VITE_APP_NAME=Dubai Real Estate AI Platform (Development)
VITE_API_URL=http://localhost:3001
VITE_CLIENT_ID=demo_client
VITE_DEFAULT_LANGUAGE=en

# ==============================================
# DATA SOURCE CONFIGURATION
# ==============================================
# Set to false to use real database, true for mock data
VITE_MOCK_DATA=false
VITE_DEBUG=true
VITE_ENVIRONMENT=development

# Database API Configuration (Backend Server)
VITE_DB_API_URL=http://localhost:3001
VITE_DB_API_TOKEN=dev_token_123

# Real Database Mode - Uses backend API server
VITE_USE_REAL_DATABASE=true
VITE_BACKEND_API_URL=http://localhost:3001

# ==============================================
# AI VOICE AGENT (VAPI) CONFIGURATION
# ==============================================
# Get your API key from https://vapi.ai
VITE_VAPI_API_KEY=your_vapi_api_key_here
VITE_VAPI_ASSISTANT_ID_EN=your_english_assistant_id
VITE_VAPI_ASSISTANT_ID_AR=your_arabic_assistant_id

# ==============================================
# WHATSAPP BUSINESS API
# ==============================================
VITE_WHATSAPP_API_TOKEN=your_whatsapp_token
VITE_WHATSAPP_PHONE_NUMBER_ID=your_phone_number_id
VITE_WHATSAPP_BUSINESS_ACCOUNT_ID=your_business_account_id
VITE_WHATSAPP_WEBHOOK_VERIFY_TOKEN=your_webhook_verify_token

# ==============================================
# EMAIL SERVICE (SENDGRID)
# ==============================================
VITE_SENDGRID_API_KEY=your_sendgrid_api_key
VITE_SENDGRID_FROM_EMAIL=noreply@yasta.online
VITE_SENDGRID_FROM_NAME=Dubai Real Estate AI Platform

# ==============================================
# N8N WORKFLOW INTEGRATION
# ==============================================
VITE_N8N_WEBHOOK_URL=https://n8n.yasta.online
VITE_N8N_API_KEY=your_n8n_api_key

# ==============================================
# BRANDING & CUSTOMIZATION
# ==============================================
VITE_BRAND_PRIMARY_COLOR=#1E40AF
VITE_BRAND_SECONDARY_COLOR=#64748B
VITE_BRAND_LOGO_URL=https://yasta.online/logo.png
VITE_COMPANY_NAME=Dubai Real Estate AI Platform

# ==============================================
# DEVELOPMENT FEATURES
